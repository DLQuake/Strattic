<div class="b-Team">
	<div class="b-Team__container">
		<div class="b-Team__list">
			{% for arg in args %}
				{% include "modules/TeamCard/TeamCard.module.twig" with {
					link: arg.url_post,
					title: arg.title_post,
					text: arg.post_excerpt,
					image: arg.post_image,
					imageAlt:  arg.title_post,
					textLayout: "textTitle",
					imageWidthNormal: 367,
					imageHeightNormal: 316,
					imageWidthSmall: 133,
					imageHeightSmall: 161,
					}%}
			{% endfor %}
		</div>
	</div>
	<div class="b-Team__rectangle"></div>
</div>
